    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Dbg app",
            "type": "node",
            "program": "",
            "cwd": "${workspaceFolder}",
            "env": {
                "DEBUG": "mmu:*"
            },
            "runtimeExecutable": "/usr/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
                "./src/app.ts",
                "-t",
                "1.21.5",
                "/home/.minecraft/home/Fabric 1.21.4/mods",
                "-o",
                "/home/.minecraft/home/Fabric 1.21.5/mods"
            ],
            "attachSimplePort": 9229
        }
    ]
}